<resources>
    <!-- Base application theme. -->
    <style name="Theme.Android_test" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#890F0D</item>
        <item name="colorPrimaryVariant">#630606</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#E83A14</item>
        <item name="colorSecondaryVariant">#E83A14</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#630606</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Android_test.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Android_test.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Android_test.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>